<resources>

    <style name="rbStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/y40</item>
        <item name="android:textSize">@dimen/y20</item>
    </style>

    <style name="backStage_title">
        <item name="android:layout_height">@dimen/y100</item>
        <item name="android:layout_width">0px</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/x60</item>
    </style>

    <style name="containerConfig_title2">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/y40</item>
        <item name="android:layout_marginTop">@dimen/y10</item>
    </style>


    <style name="rbMotorTypeStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/y50</item>
        <item name="android:textSize">@dimen/y15</item>
    </style>

    <style name="toolbar_tvStyle">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/x40</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">@dimen/x259</item>
        <item name="android:layout_height">@dimen/y49</item>
    </style>

    <style name="toolbar_btn">
        <item name="android:background">@drawable/toolbar_btn_selector</item>
        <item name="android:paddingLeft">@dimen/x20</item>
        <item name="android:paddingRight">@dimen/x20</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#636363</item>
        <item name="android:textSize">@dimen/x30</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/y60</item>
    </style>

    <style name="stockManager_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/colorTextContent</item>
        <item name="android:textSize">@dimen/x20</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxWidth">@dimen/y120</item>
    </style>

    <style name="stockManager_btn">
        <item name="android:layout_width">@dimen/x35</item>
        <item name="android:layout_height">@dimen/y35</item>
        <item name="android:background">@drawable/toolbar_btn_selector</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/colorTextContent</item>
        <item name="android:textSize">@dimen/y25</item>
    </style>

    <style name="stockManager_edit">
        <item name="android:layout_width">@dimen/x50</item>
        <item name="android:layout_height">@dimen/y35</item>
        <item name="android:gravity">center</item>
        <item name="android:inputType">number</item>
        <item name="android:textSize">@dimen/y18</item>
        <item name="android:textColorHint">@color/colorBlackMainTabText</item>
        <item name="android:textStyle">bold</item>
        <item name="android:maxLength">4</item>
        <item name="android:layout_marginStart">@dimen/x10</item>
        <item name="android:layout_marginEnd">@dimen/x10</item>
        <item name="android:background">@drawable/stockmanager_count_normal</item>
    </style>

    <style name="device_config_card">
        <item name="android:layout_width">@dimen/x300</item>
        <item name="android:layout_height">@dimen/y200</item>
        <item name="android:layout_marginStart">@dimen/x16</item>
        <item name="android:layout_marginTop">@dimen/x16</item>
        <item name="android:background">@drawable/aisle_modify_selector</item>
    </style>

    <style name="deviceConfigItemTv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/y20</item>
        <item name="android:textSize">@dimen/y30</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="deviceConfigSpinner">
        <item name="android:layout_width">@dimen/x300</item>
        <item name="android:layout_height">@dimen/y80</item>
        <item name="android:layout_marginEnd">@dimen/x30</item>
        <item name="android:spinnerMode">dropdown</item>
    </style>

    <style name="deviceConfig_Edit">
        <item name="android:textSize">@dimen/y25</item>
        <item name="android:layout_width">@dimen/x350</item>
        <item name="android:layout_height">@dimen/y80</item>
        <item name="android:inputType">number</item>
        <item name="android:layout_marginEnd">@dimen/x30</item>
    </style>

    <style name="deviceConfig_2Constraint">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginTop">@dimen/y10</item>
        <item name="android:background">@drawable/aisle_modify_selector</item>
    </style>

    <style name="checkDevice_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/y60</item>
        <item name="android:layout_marginStart">@dimen/x10</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/y30</item>
    </style>

    <style name="checkDevice_edit">
        <item name="android:layout_width">@dimen/x100</item>
        <item name="android:layout_height">@dimen/y60</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLength">3</item>
        <item name="android:textSize">@dimen/y20</item>
        <item name="android:inputType">number</item>
    </style>

    <style name="container_ll">
        <item name="android:background">@drawable/container_item_bg</item>
        <item name="android:layout_width">@dimen/x400</item>
        <item name="android:padding">@dimen/x10</item>
        <item name="android:layout_height">@dimen/y400</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <!-- login -->
    <style name="tilStyle">
        <item name="android:layout_width">@dimen/x730</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="tieStyle">
        <item name="android:textColorHint">@color/colorTv</item>
        <item name="android:textSize">@dimen/y40</item>

        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="theme">@style/customLineColor</item>
    </style>

    <style name="customLineColor" parent="@android:style/TextAppearance">
        <item name="colorControlNormal">@color/gray</item>
        <item name="colorControlActivated">@color/colorGreen</item>

    </style>

    <style name="switchBtnStyle">
        <item name="android:layout_width">@dimen/x150</item>
        <item name="android:layout_height">@dimen/y60</item>
        <item name="android:layout_marginEnd">@dimen/x30</item>
        <item name="kswTextOff">开</item>
        <item name="kswTextOn">关</item>
        <item name="android:textSize">@dimen/y25</item>
    </style>

    <style name="hori_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/x2</item>
        <item name="android:layout_marginTop">@dimen/y30</item>
        <item name="android:background">@color/gray</item>
    </style>

    <style name="backStage_modify_tv">
        <item name="android:layout_height">@dimen/y70</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/x30</item>
        <item name="android:background">@drawable/horizontal_line</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingStart">@dimen/x30</item>
    </style>

</resources>
